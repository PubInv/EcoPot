FoamFile
{
    format      ascii;
    class       polyBoundaryMesh;
    location    "constant/water/polyMesh";
    object      boundary;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

2
(
    water_to_solid
    {
        type            mappedWall;
        inGroups        List<word> 1(wall);
        nFaces          20668;
        startFace       160280;
        neighbourRegion solid;
        neighbourPatch  solid_to_water;
        transformType   none;
    }
    water_to_air
    {
        type            mappedWall;
        inGroups        List<word> 1(wall);
        nFaces          3508;
        startFace       180948;
        neighbourRegion air;
        neighbourPatch  air_to_water;
        transformType   none;
    }
)

