FoamFile
{
    format      ascii;
    class       volScalarField;
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 1e-5;//uniform 0;

boundaryField
{
    water_to_air
    {
        type            nutkWallFunction;
        value           uniform 1e-5;//uniform 0;
    }

    water_to_solid
    {
        type            nutkWallFunction;
        value           uniform 1e-5;//uniform 0;
    }

    #includeEtc "caseDicts/setConstraintTypes"
}

