dimensions      [0 0 0 1 0 0 0];

internalField   uniform $Tinitial;

boundaryField
{
    
    air2_to_solid
    {
        type            coupledTemperature;
        value           uniform $Tinitial;
    }
    
    air2_to_lid
    {
        type            coupledTemperature;
        value           uniform $Tinitial;
    }

    air2_to_water
    {
        type            coupledTemperature;
        value           uniform $Tinitial;
    }

    #includeEtc "caseDicts/setConstraintTypes"
