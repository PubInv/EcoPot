3
(
    air2_to_lid
    {
        type            mappedWall;
        inGroups        List<word> 1(wall);
        nFaces          18408;
        startFace       101534;
        neighbourRegion lid;
        neighbourPatch  lid_to_air2;
        transformType   none;
    }
    air2_to_water
    {
        type            mappedWall;
        inGroups        List<word> 1(wall);
        nFaces          5920;
        startFace       119942;
        neighbourRegion water;
        neighbourPatch  water_to_air2;
        transformType   none;
    }
    air2_to_solid
    {
        type            mappedWall;
        inGroups        List<word> 1(wall);
        nFaces          4444;
        startFace       125862;
        neighbourRegion solid;
        neighbourPatch  solid_to_air2;
        transformType   none;
    }
)

