FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0/water";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    water_to_air
    {
        type            nutkWallFunction;
        value           uniform 0;
    }
    water_to_solid
    {
        type            nutkWallFunction;
        value           uniform 0;
    }
    procBoundary2to0
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary2to1
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary2to3
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary2to6
    {
        type            processor;
        value           uniform 0;
    }
}


