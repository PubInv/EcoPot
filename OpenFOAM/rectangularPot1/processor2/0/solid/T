FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0/solid";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 298;

boundaryField
{
    solid_to_water
    {
        type            coupledTemperature;
        value           uniform 298;
    }
    solid_to_air
    {
        type            coupledTemperature;
        value           uniform 298;
    }
    procBoundary2to0
    {
        type            processor;
        value           uniform 298;
    }
    procBoundary2to1
    {
        type            processor;
        value           uniform 298;
    }
    procBoundary2to3
    {
        type            processor;
        value           uniform 298;
    }
    procBoundary2to4
    {
        type            processor;
        value           uniform 298;
    }
    procBoundary2to5
    {
        type            processor;
        value           uniform 298;
    }
    procBoundary2to7
    {
        type            processor;
        value           uniform 298;
    }
    procBoundary2to8
    {
        type            processor;
        value           uniform 298;
    }
    procBoundary2to9
    {
        type            processor;
        value           uniform 298;
    }
    procBoundary2to10
    {
        type            processor;
        value           uniform 298;
    }
}


