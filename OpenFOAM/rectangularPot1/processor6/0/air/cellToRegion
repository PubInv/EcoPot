FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0/air";
    object      cellToRegion;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            zeroGradient;
    }
    outlet
    {
        type            zeroGradient;
    }
    walls
    {
        type            zeroGradient;
    }
    air_to_water
    {
        type            extrapolatedCalculated;
        value           uniform 0;
    }
    air_to_solid
    {
        type            extrapolatedCalculated;
        value           uniform 0;
    }
    procBoundary6to1
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary6to2
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary6to3
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary6to5
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary6to7
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary6to9
    {
        type            processor;
        value           uniform 0;
    }
}


