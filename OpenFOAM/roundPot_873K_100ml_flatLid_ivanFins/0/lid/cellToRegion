FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0/lid";
    object      cellToRegion;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [];

internalField   uniform 4;

boundaryField
{
    lid_to_air2
    {
        type            calculated;
        value           uniform 0;
    }
    lid_to_air1
    {
        type            calculated;
        value           uniform 0;
    }
    lid_to_solid
    {
        type            calculated;
        value           uniform 0;
    }
}


