FoamFile
{
    format      ascii;
    class       volVectorField;
    location    "0/air2";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    air2_to_lid
    {
        type            noSlip;
    }
    air2_to_water
    {
        type            noSlip;
    }
    air2_to_solid
    {
        type            noSlip;
    }
    procBoundary1to0
    {
        type            processor;
        value           uniform (0 0 0);
    }
}


