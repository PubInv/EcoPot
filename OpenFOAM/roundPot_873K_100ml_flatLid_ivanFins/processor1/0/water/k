FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0/water";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 1e-05;

boundaryField
{
    water_to_air2
    {
        type            kqRWallFunction;
        value           uniform 1e-05;
    }
    water_to_solid
    {
        type            kqRWallFunction;
        value           uniform 1e-05;
    }
    procBoundary1to0
    {
        type            processor;
        value           uniform 1e-05;
    }
}


