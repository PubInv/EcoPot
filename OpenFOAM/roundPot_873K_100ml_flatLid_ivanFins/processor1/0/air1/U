FoamFile
{
    format      ascii;
    class       volVectorField;
    location    "0/air1";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    inlet
    {
        type            flowRateInletVelocity;
        meanVelocity    
        {
            type            constant;
            value           2;
        }
        value           nonuniform List<vector> 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform (0 0 0);
        value           uniform (0 0 0);
    }
    walls
    {
        type            noSlip;
    }
    air1_to_solid
    {
        type            noSlip;
    }
    air1_to_lid
    {
        type            noSlip;
    }
    procBoundary1to0
    {
        type            processor;
        value           uniform (0 0 0);
    }
}


