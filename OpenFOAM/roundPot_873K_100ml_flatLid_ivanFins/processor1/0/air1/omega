FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0/air1";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 2;

boundaryField
{
    inlet
    {
        type            turbulentMixingLengthFrequencyInlet;
        mixingLength    0.0035;
        value           nonuniform List<scalar> 0();
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 2;
        value           uniform 2;
    }
    walls
    {
        type            omegaWallFunction;
        value           uniform 2;
    }
    air1_to_solid
    {
        type            omegaWallFunction;
        value           uniform 2;
    }
    air1_to_lid
    {
        type            omegaWallFunction;
        value           uniform 2;
    }
    procBoundary1to0
    {
        type            processor;
        value           uniform 2;
    }
}


// ************************************************************************* //
