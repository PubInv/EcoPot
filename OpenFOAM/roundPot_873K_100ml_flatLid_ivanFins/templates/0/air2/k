FoamFile
{
    format      ascii;
    class       volScalarField;
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

kInlet          0.00001;//0;
intensityInlet  0.01;

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform $kInlet;

boundaryField
{
    
    air2_to_water
    {
        type            kqRWallFunction;
        value           uniform $kInlet;
    }
    
    air2_to_solid
    {
        type            kqRWallFunction;
        value           uniform $kInlet;
    }

    air2_to_lid
    {
        type            kqRWallFunction;
        value           uniform $kInlet;
    }

    #includeEtc "caseDicts/setConstraintTypes"
}


