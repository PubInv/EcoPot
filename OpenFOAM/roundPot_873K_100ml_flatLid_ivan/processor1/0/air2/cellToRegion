FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0/air2";
    object      cellToRegion;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [];

internalField   uniform 3;

boundaryField
{
    air2_to_lid
    {
        type            calculated;
        value           uniform 0;
    }
    air2_to_water
    {
        type            calculated;
        value           uniform 0;
    }
    air2_to_solid
    {
        type            calculated;
        value           uniform 0;
    }
    procBoundary1to0
    {
        type            processor;
        value           uniform 3;
    }
}


