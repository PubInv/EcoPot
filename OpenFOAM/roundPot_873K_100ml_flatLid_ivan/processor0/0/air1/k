FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0/air1";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.0006;

boundaryField
{
    inlet
    {
        type            turbulentIntensityKineticEnergyInlet;
        intensity       0.01;
        value           uniform 0.0006;
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 0.0006;
        value           uniform 0.0006;
    }
    walls
    {
        type            kqRWallFunction;
        value           uniform 0.0006;
    }
    air1_to_solid
    {
        type            kqRWallFunction;
        value           uniform 0.0006;
    }
    air1_to_lid
    {
        type            kqRWallFunction;
        value           uniform 0.0006;
    }
    procBoundary0to1
    {
        type            processor;
        value           uniform 0.0006;
    }
}


