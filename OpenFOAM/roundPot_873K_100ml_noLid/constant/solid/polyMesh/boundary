2
(
    solid_to_air
    {
        type            mappedWall;
        inGroups        List<word> 1(wall);
        nFaces          44732;
        startFace       188650;
        neighbourRegion air;
        neighbourPatch  air_to_solid;
        transformType   none;
    }
    solid_to_water
    {
        type            mappedWall;
        inGroups        List<word> 1(wall);
        nFaces          31436;
        startFace       233382;
        neighbourRegion water;
        neighbourPatch  water_to_solid;
        transformType   none;
    }
