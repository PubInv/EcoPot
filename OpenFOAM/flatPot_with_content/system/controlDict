FoamFile
{
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     foamMultiRun;

#includeIfPresent "regionSolvers"

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         94.5;//120;

deltaT          5e-4;

writeControl    runTime;

writeInterval   0.5;

purgeWrite      0;

writeFormat     ascii;

writePrecision  8;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

//lines below are added by Cledden to check error with simulation of negative initial temperature
//adjustTimeStep 	yes;
