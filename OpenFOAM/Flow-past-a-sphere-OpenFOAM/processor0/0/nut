FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];


internalField   uniform 3.2e-05;

boundaryField
{
    inlet
    {
        type            freestream;
        freestreamValue uniform 3.2e-05;
        value           uniform 3.2e-05;
    }
    outlet
    {
        type            freestream;
        freestreamValue uniform 3.2e-05;
        value           uniform 3.2e-05;
    }
    top
    {
        type            freestream;
        freestreamValue nonuniform List<scalar> 0();
        value           nonuniform List<scalar> 0();
    }
    bottom
    {
        type            freestream;
        freestreamValue uniform 3.2e-05;
        value           uniform 3.2e-05;
    }
    sides
    {
        type            freestream;
        freestreamValue uniform 3.2e-05;
        value           uniform 3.2e-05;
    }
    procBoundary0to1
    {
        type            processor;
        value           uniform 3.2e-05;
    }
    procBoundary0to2
    {
        type            processor;
        value           uniform 3.2e-05;
    }
}


