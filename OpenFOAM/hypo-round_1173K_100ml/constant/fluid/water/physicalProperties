FoamFile
{
    format      ascii;
    class       dictionary;
    object      physicalProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

thermoType
{
    type            heRhoThermo;
    mixture         pureMixture;
    transport       const;
    thermo          hConst;
    equationOfState perfectGas;
    specie          specie;
    energy          sensibleEnthalpy;
}

mixture
{
    specie
    {
        nMoles      1;
        molWeight   18;       // [kg/kmol]
    }
    thermodynamics
    {
        Cp          4184;       // [J/kg/K]
        hf          0;
    }
    transport
    {
        mu          8.91e-04;    // [kg/m/s]
        Pr          6.14;
    }
}


